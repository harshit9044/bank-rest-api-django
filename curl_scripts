#superuser-> username = harshit@ , password = harshit@



# Creating a new token and refresh token by providing username and password
curl -X POST -d "username=harshit@&password=harshit@" "http://localhost:8000/api/token/"

# Refreshing the access token
curl -X POST -d "refresh=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJ0b2tlbl90eXBlIjoicmVmcmVzaCIsImV4cCI6MTU2MjA3ODk2MCwianRpIjoiMTMxYjhiNzgzYTUyNDQ3ODk3MWUxNThjOWJkNWQwNTEiLCJ1c2VyX2lkIjoxfQ.vWEmukHgEp5EXAj3DfcQHPjcB55X_R_2_2_MZ-04qho" "http://localhost:8000/api/token-refresh/"

# Fetching list of API endpoints
curl "http://localhost:8000/api/"

# Fetching the details of a bank provided an ifsc code , ifsc=ABHY0065001
curl -H "Authorization: Bearer eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJ0b2tlbl90eXBlIjoiYWNjZXNzIiwiZXhwIjoxNTYxNjQ2OTYwLCJqdGkiOiI4M2E4NDkwNmVlMTA0ODljYTc2ZjkwNTY4ZThjNTRiMiIsInVzZXJfaWQiOjF9.vX_3Px6BPln40kKgbp6qqFSzCj250TzQdjHjEpDLG1c" "http://127.0.0.1:8000/api/bank-detail/?ifsc=ABHY0065001"

# Fetching the details of a bank provided an ifsc code , ifsc=ALLA0211748
curl -H "Authorization: Bearer eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJ0b2tlbl90eXBlIjoiYWNjZXNzIiwiZXhwIjoxNTYxNjQ2OTYwLCJqdGkiOiI4M2E4NDkwNmVlMTA0ODljYTc2ZjkwNTY4ZThjNTRiMiIsInVzZXJfaWQiOjF9.vX_3Px6BPln40kKgbp6qqFSzCj250TzQdjHjEpDLG1c" "http://127.0.0.1:8000/api/bank-detail/?ifsc=ALLA0211748"


# Fetching Bank list with city=MUMBAI ,bank-name=ABHYUDAYA COOPERATIVE BANK LIMITED ,limit=6 and offset=2
curl -H "Authorization: Bearer eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJ0b2tlbl90eXBlIjoiYWNjZXNzIiwiZXhwIjoxNTYxNjQ2OTYwLCJqdGkiOiI4M2E4NDkwNmVlMTA0ODljYTc2ZjkwNTY4ZThjNTRiMiIsInVzZXJfaWQiOjF9.vX_3Px6BPln40kKgbp6qqFSzCj250TzQdjHjEpDLG1c" "http://127.0.0.1:8000/api/all-banks/?bank-name=ABHYUDAYA%20COOPERATIVE%20BANK%20LIMITED&city=MUMBAI&limit=6&offset=2"

# Fetching Bank list with city=MUMBAI ,bank-name=ABHYUDAYA COOPERATIVE BANK LIMITED ,limit=3 and offset=0
